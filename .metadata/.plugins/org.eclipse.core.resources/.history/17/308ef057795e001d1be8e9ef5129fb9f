package programacion1;

import java.util.Scanner;

public class ejercicio1Tema4 {

	public static void main(String[] args) {
	Scanner teclado= new Scanner(System.in);
		int texto;
		System.out.print("Introduzca una frase de menos de 80 palabras: ");
		texto=teclado.nextInt();
		while(texto<80)
			for(int i=0; i<texto; i++) {
				System.out.print("");
				for(int contador=i; contador<i; contador++) {
					System.out.print(contador);
				}
					
			}
				
			
			
	}
}